        <script>
        function openbar() {
        document.getElementById("menubar").style.width = "100px";
        
        document.getElementById("categories").style.marginLeft = "50px";

        }

        function closebar() {
             document.getElementById("menubar").style.width = "0";
             
             document.getElementById("categories").style.marginLeft= "0";
        }
        let slideIndex = 0;
        let slides = document.getElementsByClassName("Add");
        showSlides();
        function showSlides() {
                 let i;
                 let dots = document.getElementsByClassName("dot");
            for (i = 0; i < slides.length; i++) {
                      slides[i].style.display = "none";  
            }
               slideIndex++;
             if (slideIndex > slides.length) {slideIndex = 1}    
            for (i = 0; i < dots.length; i++) {
                dots[i].className = dots[i].className.replace(" active", "");
               }
                slides[slideIndex-1].style.display = "block";  
             dots[slideIndex-1].className += " active";
             setTimeout(showSlides, 2000); // Change image every 2 seconds
            }
            function prev(){
                let c=slideIndex-1;
                if(c<1){
                    c=slides.length-1;
                }
                for (i = 0; i < slides.length; i++) {
                      slides[i].style.display = "none";  
                }

                slides[c].style.display="block";

               slideIndex=c;
            }
            function next(){
                let n=slideIndex+1;
                if(n>slides.lenth){
                    n=1;
                }
                for (i = 0; i < slides.length; i++) {
                      slides[i].style.display = "none";  
                }
                slides[n].style.display="block";
                slideIndex=n;
            }

var countDownDate = new Date("jan 30, 2023 12:30:00").getTime();

var x = setInterval(function() {

  var now = new Date().getTime();
    
  var distance = countDownDate - now;

  var hours = Math.floor((distance % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
  var minutes = Math.floor((distance % (1000 * 60 * 60)) / (1000 * 60));
  var seconds = Math.floor((distance % (1000 * 60)) / 1000);
    
  document.getElementById("timer").innerHTML = hours + ": "
  + minutes + ": " + seconds+" left";
  if (distance < 0) {
    clearInterval(x);
    document.getElementById("timer").innerHTML = "EXPIRED";
  }
}, 1000);
            

            
    
      
        </script>
    </body>
